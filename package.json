{
  "name": "opennote-api",
  "version": "1.0.0",
  "description": "Backend for OpenNote",
  "main": "js/index.js",
  "scripts": {
    "start": "node --inspect=5858 -r ts-node/register ./ts/index.ts",
    "start:watch": "nodemon",
    "build": "tsc -p ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EricRabil/opennote-api.git"
  },
  "author": "Eric Rabil <ericjrabil@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EricRabil/opennote-api/issues"
  },
  "homepage": "https://github.com/EricRabil/opennote-api#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "url-metadata": "^2.4.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.3",
    "@types/url-metadata": "^2.1.0",
    "nodemon": "^2.0.2",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "ts"
    ],
    "exec": "npm start",
    "ext": "ts"
  }
}
